{
  "manifest_version": 2,

  "name": "SafeSaving",
  "description": "Save files to xsafesaving",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["main.js"]
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "notifications",
    "*://safesaving.tk/*"
  ]
}